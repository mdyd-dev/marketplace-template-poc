{% liquid
  function current_user = 'lib/current_profile', user_id: context.current_user.id, required: true
  include 'lib/validate_profile_completeness', profile: current_user

  function list = 'lib/data/queries/activities/search', feed: 'user-private', feed_id: current_user.id, limit: 100

  include 'theme/simple/activities/feed/index', list: list, current_user: current_user
  render 'theme/simple/items/follow.js', authenticity_token: context.authenticity_token
%}

