{% liquid
  include 'lib/must_be_authenticated'
  function current_user = 'lib/data/queries/profiles/find', user_id: context.current_user.id

  function groups = 'lib/data/queries/groups/search', limit: 100, owner_id: current_user.id

  include 'theme/simple/dashboard/breadcrumbs', name: 'groups', total_entries: groups.total_entries, role: 'seller'
  include 'theme/simple/dashboard/groups/index', groups: groups
%}
