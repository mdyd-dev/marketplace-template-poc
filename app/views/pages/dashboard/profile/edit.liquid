{% liquid
  include 'lib/must_be_authenticated'

  function current_user = 'lib/data/queries/users/find', id: context.current_user.id
  function profile = 'lib/data/queries/profiles/find', user_uuid: current_user.uuid
  graphql presigned_url = 'photos/presign' | dig: 'presigned'

  render 'theme/simple/dashboard/profile/edit', object: profile, presigned_url: presigned_url, context: context
%}
