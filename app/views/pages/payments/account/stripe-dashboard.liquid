{% liquid
  include 'modules/stripe/lib/payments/account'
  assign account = context.exports.account

  if account.id
    function_rc login_link_url = 'modules/stripe/lib/account/login_link/call', account: account
    log login_link_url, type: 'login_link_url'
    redirect_to login_link_url
  else
    redirect_to '/payments/account'
  endif
%}
