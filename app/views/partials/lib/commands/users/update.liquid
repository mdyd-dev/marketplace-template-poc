{% liquid
  function_rc object = 'lib/commands/users/update/build', object: params, user: user
  function_rc object = 'lib/commands/users/update/check', object: object

  if object.valid
    function_rc object = 'lib/commands/users/update/execute', object: object

    if object.valid
      include 'lib/commands/events/create', type: 'account_updated', actor_id: object.id
    endif
  endif

  return_rc object
%}
